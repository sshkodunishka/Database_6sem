        <th>
            @Html.DisplayNameFor(model => model.pay)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vouchers.voucherName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
